{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "types": ["vite/client"],
    "lib": ["esnext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  },
  "include": ["src"]
}